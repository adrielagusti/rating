{
  "_version": "1.45.0",
  "sap.app": {
    "id": "blackseeds.ratings",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.1.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.8.0",
      "toolsId": "801aa4e6-d929-497c-b53e-559762d512d0"
    },
    "dataSources": {
      "mainService": {
        "uri": "/v2/black-seeds/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "dependencies": {
      "minUI5Version": "1.109.3",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "blackseeds.ratings.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "defaultBindingMode": "TwoWay",
          "defaultCountMode": "Inline"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "blackseeds.ratings.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false,
        "path": "blackseeds.ratings.view"
      },
      "routes": [
        {
          "pattern": "",
          "name": "main",
          "target": ["main"]
        },
        {
          "pattern": "ratingSpecimen/{objectId}",
          "name": "ratingSpecimen",
          "target": ["rating"]
        },
        {
          "pattern": "ratingStrain/{objectId}",
          "name": "ratingStrain",
          "target": ["rating"]
        },
        {
          "pattern": "collection",
          "name": "collection",
          "target": ["collection"]
        },
        {
          "pattern": "specimen/{objectId}",
          "name": "specimen",
          "target": ["specimen"]
        }
      ],
      "targets": {
        "rating": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewLevel": 2,
          "viewId": "Rating",
          "viewName": "Rating"
        },
        "main": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewLevel": 1,
          "viewId": "main",
          "viewName": "Main"
        },
        "collection": {
          "viewType": "XML",
          "transition": "flip",
          "clearControlAggregation": false,
          "viewLevel": 2,
          "viewId": "Collection",
          "viewName": "Collection"
        },
        "specimen": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewLevel": 3,
          "viewId": "Specimen",
          "viewName": "Specimen"
        }
      }
    },
    "rootView": {
      "viewName": "blackseeds.ratings.view.Main",
      "type": "XML",
      "async": true,
      "id": "Main"
    },
    "extends": {
      "extensions": {
        "sap.ui.controllerExtensions": {}
      }
    }
  }
}
